local GotoSection = TPTab:CreateSection({Name = "Player Teleports", Side = "Left"})
GotoSection:AddTextbox(
    {
        Name = "Go To",
        Value = "",
        Placeholder = "Enter Username/Display",
        Callback = function(v)
            local TeleportTarget = FindPlayer(v)
            if TeleportTarget and TeleportTarget.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                if Revival.PTele.Type == "Tween" then
                    TweenService:Create(LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Revival.PTele.Speed, Enum.EasingStyle.Linear), {CFrame = TeleportTarget.Character.HumanoidRootPart.CFrame}):Play()
                elseif Revival.PTele.Type == "CFrame" then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = TeleportTarget.Character.HumanoidRootPart.CFrame
                end
            end
        end
    }
)
GotoSection:AddDropdown(
    {
        Name = "Teleport Type",
        Value = Revival.PTele.Type,
        List = {"Tween", "CFrame"},
        Callback = function(v)
            Revival.PTele.Type = v
            Notify("Revival | Teleport", "Teleport Type : "..tostring(Revival.PTele.Type), 3)
        end
    }
)
GotoSection:AddSlider(
    {
        Name = "Teleport Speed",
        Value = Revival.PTele.Speed,
        Min = 0,
        Max = 5,
        Format = function(v)
            return "Speed: " .. tostring(v)
        end,
        Callback = function(v)
            Revival.PTele.Speed = v
            Notify("Revival | Teleport", "Speed: " .. tostring(Revival.PTele.Speed), 3)
        end
    }
)



local MiscTab = Revival:CreateTab({Name = "Misc"})
local MiscSection = MiscTab:CreateSection({Name = "Misc", Side = "Left"})
MiscSection:AddSlider(
    {
        Name = "Field Of View",
        Value = workspace.CurrentCamera.FieldOfView,
        Min = 70,
        Max = 120,
        Format = function(v)
            return "Field Of View: " .. tostring(v)
        end,
        Callback = function(v)
            workspace.CurrentCamera.FieldOfView = v
            Notify("Revival | Field Of View", "Field Of View: " .. tostring(workspace.CurrentCamera.FieldOfView), 3)
        end
    }
)
MiscSection:AddToggle(
    {
        Name = "Show Chat",
        Value = Revival.Visuals.Chat,
        Callback = function(v)
            Revival.Visuals.Chat = v
            LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame.Visible = Revival.Visuals.Chat
            LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame.Position = UDim2.new(0, 0, 0, 160)
            Notify("Revival | Notification", "Chat: " .. tostring(Revival.Visuals.Chat), 3)
        end
    }
)
MiscSection:AddToggle(
    {
        Name = "Headless",
        Value = Revival.Headless.Enabled,
        Callback = function(v)
            Revival.Headless.Enabled = v
            Notify("Revival | Headless", "Headless : "..tostring(Revival.Headless.Enabled), 3)
            if Revival.Headless.Enabled then
                LocalPlayer.Character.Head.Transparency = 1
                    for i, v in pairs(LocalPlayer.Character.Head:GetChildren()) do 
                        if (v:IsA('Decal')) then
                            v.Transparency = 1
                        end
                    end
            else
                LocalPlayer.Character.Head.Transparency = 0
                for i, v in pairs(LocalPlayer.Character.Head:GetChildren()) do 
                    if (v:IsA('Decal')) then
                        v.Transparency = 0
                    end
                end
            end
        end
    }
)
MiscSection:AddToggle(
    {
        Name = "No Recoil",
        Value = Revival.NoRecoil.Enabled,
        Callback = function(v)
            Revival.NoRecoil.Enabled = v
            Notify("Revival | No Recoil", "No Recoil: " .. tostring(Revival.NoRecoil.Enabled), 3)
        end
    }
)
MiscSection:AddToggle(
    {
        Name = "No JumpCooldown",
        Value = Revival.NoJumpcooldown.Enabled,
        Callback = function(v)
            Revival.NoJumpcooldown.Enabled = v
            Notify("Revival | No Jump Cooldown", "No Jump Cooldown: " .. tostring(Revival.NoJumpcooldown.Enabled), 3)
        end
    }
)
MiscSection:AddToggle(
    {
        Name = "Notifications",
        Value = Revival.Notification.Enabled,
        Callback = function(v)
            Revival.Notification.Enabled = v
            Notify("Revival | Notification", "Notifications: " .. tostring(Revival.Notification.Enabled), 3)
        end
    }
)
