local SilentFOV = Drawing.new("Circle")
SilentFOV.Visible = false
SilentFOV.Radius = Revival.SilentAim.Size
SilentFOV.Color = Revival.SilentAim.Color
SilentFOV.Filled = Revival.SilentAim.Filled
SilentFOV.Transparency = Revival.SilentAim.Transparency

local function FindPlayer(name)
    for _, v in pairs(Players:GetPlayers()) do 
        if string.lower(string.sub(v.Name, 1, string.len(name))) == string.lower(name) then 
            return v
        elseif string.lower(string.sub(v.DisplayName, 1, string.len(name))) == string.lower(name) then 
            return v
        end
    end
end

local function CheckTarget(player)
    if Revival.Aimlock.KnockedCheck or Revival.Camlock.KnockedCheck or Revival.SilentAim.KnockedCheck then
        if player.Character.BodyEffects:FindFirstChild("K.O").Value then return true end
    end
    if Revival.Aimlock.GrabbedCheck or Revival.Camlock.GrabbedCheck or Revival.SilentAim.GrabbedCheck then
        if player.Character:FindFirstChild("GRABBING_CONSTRAINT") then return true end
    end
    return false
end

local function GetClosestPlayer()
    local closestPlayer = nil
    local shortestDistance = math.huge

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local character = player.Character
            local rootPart = character and character:FindFirstChild("HumanoidRootPart")

            if rootPart and character:FindFirstChild("Humanoid")and not CheckTarget(player) then
                local pos = workspace.CurrentCamera:WorldToViewportPoint(rootPart.Position)
                local magnitude = (Vector2.new(pos.X, pos.Y) - Vector2.new(LocalPlayer:GetMouse().X, LocalPlayer:GetMouse().Y + 36)).Magnitude

                if magnitude < shortestDistance then
                    closestPlayer = player
                    shortestDistance = magnitude
                end
            end
        end
    end

    return closestPlayer
end

local function GetSilentPlayer(radius)
    local closestPlayer = nil
    local shortestDistance = radius

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            local character = player.Character
            local rootPart = character and character:FindFirstChild("HumanoidRootPart")

            if rootPart and character:FindFirstChild("Humanoid") and not CheckTarget(player) then
                local pos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(rootPart.Position)
                local magnitude = (Vector2.new(pos.X, pos.Y) - InputService:GetMouseLocation()).Magnitude

                if magnitude < shortestDistance and onScreen then
                    closestPlayer = player
                    shortestDistance = magnitude
                end
            end
        end
    end

    return closestPlayer
end

local function Notify(title, text, time)
    if Revival.Notification.Enabled then
        NotifyLibrary.Notify({
            Title = title;
            Description = text;
            Duration = time;
        })
    else
        print(title, text)
    end
end

local function Fly()
    Revival.Fly.Enabled = not Revival.Fly.Enabled
    Notify("Revival | Fly", "Flying : " .. tostring(Revival.Fly.Enabled), 3)
    local BodyPosition = Instance.new("BodyPosition", game.Players.LocalPlayer.Character:FindFirstChild("Head"))
    local BodyGyro = Instance.new("BodyGyro", game.Players.LocalPlayer.Character:FindFirstChild("Head"))
    BodyPosition.maxForce = Vector3.new(math.huge, math.huge, math.huge)
    BodyPosition.position = game.Players.LocalPlayer.Character:FindFirstChild("Head").Position
    BodyGyro.maxTorque = Vector3.new(9e9, 9e9, 9e9)
    BodyGyro.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Head").CFrame
    repeat
        wait()
        game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
        local New = BodyGyro.CFrame - BodyGyro.CFrame.p + BodyPosition.position
        if InputService:IsKeyDown("W") then
            New = New + workspace.CurrentCamera.CoordinateFrame.lookVector * Revival.Fly.Speed
            BodyGyro.CFrame = workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad(Revival.Fly.Speed * 0), 0, 0)
        end
        if InputService:IsKeyDown("S") then
            New = New - workspace.CurrentCamera.CoordinateFrame.lookVector * Revival.Fly.Speed
            BodyGyro.CFrame = workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(math.rad(Revival.Fly.Speed * 0), 0, 0)
        end
        if InputService:IsKeyDown("D") then
            New = New * CFrame.new(Revival.Fly.Speed, 0, 0)
            BodyGyro.CFrame = workspace.CurrentCamera.CoordinateFrame
        end
        if InputService:IsKeyDown("A") then
            New = New * CFrame.new(-Revival.Fly.Speed, 0, 0)
            BodyGyro.CFrame = workspace.CurrentCamera.CoordinateFrame
        end
        BodyPosition.Position = New.p
    until not Revival.Fly.Enabled
    Revival.Fly.Enabled = false
    BodyGyro:Destroy()
    BodyPosition:Destroy()
    game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
end


RunService.Heartbeat:Connect(function()

    if Revival.SilentAim.Enabled and SilentFOV.Visible then
        SilentFOV.Position = InputService:GetMouseLocation()
        SilentFOV.Radius = Revival.SilentAim.Size
        SilentFOV.Transparency = Revival.SilentAim.Transparency
        SilentFOV.Color = Revival.SilentAim.Color
        SilentFOV.Filled = Revival.SilentAim.Filled
        Revival.SilentAim.Target = GetSilentPlayer(SilentFOV.Radius)
    end

    if Revival.Camlock.Enabled and Revival.Target then
        local targetPart = Revival.Camlock.Part
        if Revival.Camlock.AirShot and Revival.Target.Character:FindFirstChild("Humanoid") and Revival.Target.Character.Humanoid:GetState() == Enum.HumanoidStateType.Freefall then
            targetPart = Revival.Camlock.AirPart
        end

        local targetPosition = Revival.Target.Character[targetPart].Position

        if Revival.Camlock.Resolver then
            targetPosition = Revival.Target.Character[targetPart].Position
        elseif Revival.Camlock.Prediction then
            local velocity = Revival.Target.Character[targetPart].Velocity
            if velocity then
                targetPosition = Revival.Target.Character[targetPart].Position + (velocity * Revival.Camlock.Pred)
            end
        end

        if Revival.Camlock.Smoothness then
            TweenService:Create(
                workspace.CurrentCamera,
                TweenInfo.new(
                    Revival.Camlock.SmoothnessAmount,
                    Enum.EasingStyle[Revival.Camlock.EasingStyle],
                    Enum.EasingDirection[Revival.Camlock.EasingDirection]
                ),
                {CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, targetPosition)}
            ):Play()
        else
            workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, targetPosition)
        end
    end

    if Revival.Strafe.Enabled then
        if Revival.Target and Revival.Target.Character and Revival.Target.Character:FindFirstChild("HumanoidRootPart") then
            Revival.Strafe.Angle = Revival.Strafe.Angle + Revival.Strafe.Speed

            if Revival.Strafe.View then
                game.Workspace.CurrentCamera.CameraSubject = Revival.Target.Character.Humanoid
            else
                game.Workspace.CurrentCamera.CameraSubject = LocalPlayer.Character.Humanoid
            end

            if Revival.Strafe.Type == "Orbit" then
                LocalPlayer.Character.HumanoidRootPart.CFrame = Revival.Target.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, Revival.Strafe.Angle, 0) * CFrame.new(0, 0, Revival.Strafe.Distance)
            end
        else
            game.Workspace.CurrentCamera.CameraSubject = LocalPlayer.Character.Humanoid
        end
    else
        game.Workspace.CurrentCamera.CameraSubject = LocalPlayer.Character.Humanoid
    end

    if Revival.AutoArmor.Enabled and Revival.AutoArmor.TP and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        if LocalPlayer.Character.BodyEffects.Armor.Value <= Revival.AutoArmor.BuyHP and workspace.Ignored.Shop:FindFirstChild(Revival.AutoArmor.ArmorToBuy) and not LocalPlayer.Character.BodyEffects["K.O"].Value and not LocalPlayer.Character.BodyEffects["Grabbed"].Value then
            if not OldPos then
                OldPos = LocalPlayer.Character.HumanoidRootPart.CFrame
            end

            local equippedTool = LocalPlayer.Character:FindFirstChildOfClass("Tool")
            if equippedTool then
                equippedTool.Parent = LocalPlayer.Backpack
            end

            local shopItem = workspace.Ignored.Shop:FindFirstChild(Revival.AutoArmor.ArmorToBuy)
            if shopItem and shopItem:FindFirstChild("ClickDetector") then
                LocalPlayer.Character.HumanoidRootPart.CFrame = shopItem.Head.CFrame
                wait(Revival.AutoArmor.Delay)
                fireclickdetector(shopItem.ClickDetector)
            end

            if OldPos then
                LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
                OldPos = nil
            end
        end
    end

    if Revival.AntiAim.Enabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local OldVelocity = LocalPlayer.Character.HumanoidRootPart.Velocity
        if Revival.AntiAim.Type == "Sky" then
            LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 9999, 0)
        elseif Revival.AntiAim.Type == "Underground" then
            LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -9999, 0)
        elseif Revival.AntiAim.Type == "Custom Vel"  then
            LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(Revival.AntiAim.XValue, Revival.AntiAim.YValue, Revival.AntiAim.ZValue)
        end
        RunService.RenderStepped:Wait()
        LocalPlayer.Character.HumanoidRootPart.Velocity = OldVelocity
    end
    
end)


local Old; Old = hookmetamethod(game, "__namecall", function(self, ...)
    local args = { ... }
    local method = getnamecallmethod()

    if method == "FireServer" then
        if args[1] == "UpdateMousePos" or args[1] == "MousePos" or args[1] == "Mouse" or args[1] == "MOUSE" then
            if Revival.Aimlock.Enabled and Revival.Target then
                if Revival.Aimlock.Resolver then
                    args[2] = Revival.Target.Character[Revival.Aimlock.Part].Position
                    return Old(self, unpack(args))
                elseif Revival.Aimlock.Prediction then
                    args[2] = Revival.Target.Character[Revival.Aimlock.Part].Position + (Revival.Target.Character[Revival.Aimlock.Part].Velocity * Revival.Aimlock.Pred)
                    return Old(self, unpack(args))
                elseif Revival.Aimlock.AirShot and Revival.Target.Character:FindFirstChild("Humanoid") and Revival.Target.Character.Humanoid.FloorMaterial == Enum.Material.Air then
                    args[2] = Revival.Target.Character[Revival.Aimlock.AirPart].Position + (Revival.Target.Character[Revival.Aimlock.AirPart].Velocity * Revival.Aimlock.Pred)
                    return Old(self, unpack(args))
                else
                    args[2] = Revival.Target.Character[Revival.Aimlock.Part].Position
                    return Old(self, unpack(args))
                end
            end
            if Revival.SilentAim.Enabled and Revival.SilentAim.Target then
                if Revival.SilentAim.Prediction then
                    args[2] = Revival.SilentAim.Target.Character[Revival.SilentAim.Part].Position + (Revival.SilentAim.Target.Character[Revival.SilentAim.Part].Velocity * Revival.SilentAim.Pred)
                    return Old(self, unpack(args))
                else
                    args[2] = Revival.SilentAim.Target.Character[Revival.SilentAim.Part].Position
                    return Old(self, unpack(args))
                end
            end
        end
    end

    return Old(self, ...)
end)



local AimTab = Revival:CreateTab({Name = "Aiming"})
local AimingSection = AimTab:CreateSection({Name = "Target Aimlock", Side = "Left"})
AimingSection:AddKeybind(
    {
        Name = "Keybind",
        Keybind = "C",
        Pressed = function()
            if Revival.Target then
                Revival.Target = nil
                Notify("Revival | Target", "Targets cleared", 3)
            else
                Revival.Target = GetClosestPlayer()
                if Revival.Target then
                    Notify("Revival | Target", "Target acquired: " ..tostring(Revival.Target), 3)
                else
                    Notify("Revival | Target", "No valid targets found", 3)
                end
            end
        end
    }
)
