local CameraPrediction = CameraSection:AddSlider(
    {
        Name = "Camera Prediction",
        Value = Revival.Camlock.Pred,
        Min = 0,
        Max = 1,
        Decimals = 3,
        Format = function(v)
            return "Prediction: " .. tostring(v)
        end,
        Callback = function(v)
            Revival.Camlock.Pred = v
            Notify("Revival | Camlock", "Prediction : " .. tostring(Revival.Camlock.Prediction), 3)
        end
    }
)
CameraSection:AddToggle(
    {
        Name = "Auto Prediction",
        Value = Revival.Camlock.AutoPrediction,
        Callback = function(v)
            Revival.Camlock.AutoPrediction = v
            Notify("Revival | Camlock", "Auto Prediction : "..tostring(Revival.Camlock.AutoPrediction), 3)
            repeat
                if Revival.Camlock.AutoPrediction then
                    local ping = tonumber((game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString() or ""):split(" ")[1])
                    if ping then
                        if ping > 200 and ping < 300 then 
                            Revival.Camlock.Pred = 0.18742
                        elseif ping > 180 and ping < 195 then 
                            Revival.Camlock.Pred = 0.16779123
                        elseif ping > 140 and ping < 180 then 
                            Revival.Camlock.Pred = 0.16
                        elseif ping > 110 and ping < 140 then 
                            Revival.Camlock.Pred = 0.15934
                        elseif ping < 105 and ping > 90 then
                            Revival.Camlock.Pred = 0.138
                        elseif ping < 90 and ping > 80 then
                            Revival.Camlock.Pred = 0.136
                        elseif ping < 80 and ping > 70 then
                            Revival.Camlock.Pred = 0.134
                        elseif ping < 70 and ping > 60 then
                            Revival.Camlock.Pred = 0.131
                        elseif ping < 60 and ping > 50 then
                            Revival.Camlock.Pred = 0.1229
                        elseif ping < 50 and ping > 40 then
                            Revival.Camlock.Pred = 0.1225
                        elseif ping < 40 then
                            Revival.Camlock.Pred = 0.1256
                        end
                        CameraPrediction:RawSet(Revival.Camlock.Pred)
                    end
                end
                wait()
            until not Revival.Camlock.AutoPrediction
        end
    }
)
CameraSection:AddToggle(
    {
        Name = "Knocked Check",
        Value = Revival.Camlock.KnockedCheck,
        Callback = function(v)
            Revival.Camlock.KnockedCheck = v
            Notify("Revival | Camlock", "Knocked Check : "..tostring(Revival.Camlock.KnockedCheck), 3)
        end
    }
)
