local RageSection = CharTab:CreateSection({Name = "Rage", Side = "Left"})
RageSection:AddKeybind(
    {
        Name = "Sprint Key",
        Value = Enum.KeyCode[Revival.Speed.SprintKey],
        Mode = "Held",
        Pressed = function()
            repeat
                if Revival.Speed.Enabled then
                    if InputService:IsKeyDown("W") or InputService:IsKeyDown("A") or InputService:IsKeyDown("S") or InputService:IsKeyDown("D") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + LocalPlayer.Character.Humanoid.MoveDirection * Revival.Speed.Speed
                    end
                end
                game:GetService("RunService").Heartbeat:wait()
            until not InputService:IsKeyDown(Revival.Speed.SprintKey)
        end,
    }
)
RageSection:AddToggle(
    {
        Name = "Speed",
        Value = Revival.Speed.Enabled,
        Keybind = "",
        Callback = function(v)
            Revival.Speed.Enabled = v
            Notify("Revival | Speed", "Speed : " .. tostring(Revival.Speed.Enabled), 3)
        end
    }
)
RageSection:AddToggle(
    {
        Name = "Spin Bot",
        Value = Revival.SpinBot.Enabled,
        Callback = function(v)
            Revival.SpinBot.Enabled = v
            if Revival.SpinBot.Enabled then
                repeat
                    if LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(Revival.SpinBot.Speed), 0)
                    end
                    RunService.RenderStepped:Wait()
                until not Revival.SpinBot.Enabled
            end
            Notify("Revival | Spin bot", "Spin bot : " .. tostring(Revival.SpinBot.Enabled), 3)
        end
    }
)
