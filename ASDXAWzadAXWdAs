local AimlockPrediction = AimingSection:AddSlider(
    {
        Name = "Aimbot Prediction",
        Value = Revival.Aimlock.Pred,
        Min = 0,
        Max = 1,
        Decimals = 3,
        Format = function(v)
            return "Prediction: " .. tostring(v)
        end,
        Callback = function(v)
            Revival.Aimlock.Pred = v
            Notify("Revival | Aimlock", "Prediction: " .. tostring(Revival.Aimlock.Pred), 3)
        end
    }
)
AimingSection:AddToggle(
    {
        Name = "Auto Prediction",
        Value = Revival.Aimlock.AutoPrediction,
        Callback = function(v)
            Revival.Aimlock.AutoPrediction = v
            Notify("Revival | Aimlock", "Auto Prediction : "..tostring(Revival.Aimlock.AutoPrediction), 3)
            repeat
                if Revival.Aimlock.AutoPrediction then
                    local ping = tonumber((game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString() or ""):split(" ")[1])
                    if ping then
                        if ping > 200 and ping < 300 then 
                            Revival.Aimlock.Pred = 0.18742
                        elseif ping > 180 and ping < 195 then 
                            Revival.Aimlock.Pred = 0.16779123
                        elseif ping > 140 and ping < 180 then 
                            Revival.Aimlock.Pred = 0.16
                        elseif ping > 110 and ping < 140 then 
                            Revival.Aimlock.Pred = 0.15934
                        elseif ping < 105 and ping > 90 then
                            Revival.Aimlock.Pred = 0.138
                        elseif ping < 90 and ping > 80 then
                            Revival.Aimlock.Pred = 0.136
                        elseif ping < 80 and ping > 70 then
                            Revival.Aimlock.Pred = 0.134
                        elseif ping < 70 and ping > 60 then
                            Revival.Aimlock.Pred = 0.131
                        elseif ping < 60 and ping > 50 then
                            Revival.Aimlock.Pred = 0.1229
                        elseif ping < 50 and ping > 40 then
                            Revival.Aimlock.Pred = 0.1225
                        elseif ping < 40 then
                            Revival.Aimlock.Pred = 0.1256
                        end
                        AimlockPrediction:RawSet(Revival.Aimlock.Pred)
                    end
                end
                wait()
            until not Revival.Aimlock.AutoPrediction
        end
    }
)
AimingSection:AddToggle(
    {
        Name = "Knocked Check",
        Value = Revival.Aimlock.KnockedCheck,
        Callback = function(v)
            Revival.Aimlock.KnockedCheck = v
            Notify("Revival | Aimlock", "Knocked Check : "..tostring(Revival.Aimlock.KnockedCheck), 3)
        end
    }
)
AimingSection:AddToggle(
